<html>
<link href="/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
<link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
     rel="stylesheet">
<!-- Custom styles for this template-->
<link href="/css/sb-admin-2.min.css" rel="stylesheet">

</head>

<body class="bg-gradient-primary">

     <div class="container">

          <div class="card o-hidden border-0 shadow-lg my-5">
               <div class="card-body p-0">
                    <form class="row" action="/destination_cate/Edit_dest" method="post">
                         <input type="hidden" name="Id" value="<%=dest_cate.id%>" id="">
                         <!-- Nested ow within Card Body -->
                         <div class="d-flex w-100">
                              <ul class="navbar-nav bg-gradient-primary sidebar sidebar-dark accordion"
                                   id="accordionSidebar">

                                   <hr class="sidebar-divider my-0">
                                   <hr class="sidebar-divider">
                                   <li class="nav-item">
                                        <a href="#"
                                             class="text-decoration-none text-dark px-5 py-3 bg-light w-100 d-block">Update
                                             Destination Category</a>
                                   </li>
                                   <hr class="sidebar-divider d-none d-md-block">
                              </ul>
                              <div class="p-5" style="width: 70%;">
                                   <div class="mb-5">
                                        <h3>Update Destination Category </h3>
                                   </div>
                                   <div class="card-body">
                                        <div class="form-group row">
                                             <label class="col-sm-3 text-end control-label col-form-label">Destination
                                                  Category</label>
                                             <div class="col-sm-9">
                                                  <input type="text" name="destination_cate" class="form-control"
                                                       id="name" placeholder="Enter destination"
                                                       value="<%=dest_cate.destination_cate%>" />
                                             </div>
                                        </div>

                                        <br>
                                        <input type="submit"
                                             class="d-none d-sm-inline-block btn btn-sm btn-primary shadow-sm p-2"
                                             value="Submit"></input>
                                   </div>
                              </div>
                         </div>
                    </form>
               </div>
          </div>
     </div>
</body>

</html>
<!-- Bootstrap core JavaScript-->
<script src="/vendor/jquery/jquery.min.js"></script>
<script src="/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

<!-- Core plugin JavaScript-->
<script src="/vendor/jquery-easing/jquery.easing.min.js"></script>

<!-- Custom scripts for all pages-->
